{
  "workbench.editor.enablePreviewFromCodeNavigation": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "off"
  },
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true,
    "**/.git": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "examples/**": true,
    "docs/**": true,
    "issues/**": true
  },
  "workbench.colorTheme": "Orange Theme",
  "task.allowAutomaticTasks": "on",
  "windmillcode-open-related-files": {
    "options": [
      {
        "name": "Pluralsight side by side course",
        "searchPaths": [
          "project/mobile",
          "project/mobileFinal"
        ],
        "subStringRemovalArray":[
          "project",
          "mobile",
          "mobileFinal"
        ],
        "setEditorLayout": {
          "orientation": 1,
          "groups": [
            {
              "groups": [
                {},
                {}
              ],
              "size": 0.5
            }
          ]
        },
        "includeGlobs": [
          [
            [
              "**/project/mobile/**/FILE_NAME_BASIS"
            ],
            [
              "**/project/mobileFinal/**/FILE_NAME_BASIS"
            ]
          ]
        ]
      }
    ]
  },
  "windmillcode-open-related-files.defaultOption": {
    "name": "Pluralsight side by side course",
    "searchPaths": [
      "project/mobile",
      "project/mobileFinal"
    ],
    "subStringRemovalArray": [
      "project",
      "mobile",
      "mobileFinal"
    ],
    "setEditorLayout": {
      "orientation": 1,
      "groups": [
        {
          "groups": [
            {},
            {}
          ],
          "size": 0.5
        }
      ]
    },
    "includeGlobs": [
      [
        [
          {
            "_relativeFilePath": "",
            "filePath": "**/project/mobile/**/FILE_NAME_BASIS",
            "section": [
              0,
              0,
              0,
              0
            ],
            "createFileRelativeToTrustedFilePath": false
          }
        ],
        [
          {
            "_relativeFilePath": "",
            "filePath": "**/project/mobileFinal/**/FILE_NAME_BASIS",
            "section": [
              0,
              0,
              0,
              0
            ],
            "createFileRelativeToTrustedFilePath": false
          }
        ]
      ]
    ]
  },
  "windmillcode-open-related-files.autoOpen": true,
  "windmillcode-open-related-files.resetLayout": true
}